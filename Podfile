# platform :ios, '9.0'

target 'Map' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Map
  pod ‘GoogleMaps’
  pod ‘GooglePlaces’
  pod 'GooglePlacePicker'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  
  target 'MapTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
